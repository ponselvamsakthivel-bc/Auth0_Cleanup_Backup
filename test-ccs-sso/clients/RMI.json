{
  "is_token_endpoint_ip_header_trusted": false,
  "name": "RMI",
  "is_first_party": true,
  "oidc_conformant": true,
  "sso_disabled": false,
  "cross_origin_auth": false,
  "refresh_token": {
    "expiration_type": "expiring",
    "leeway": 0,
    "infinite_token_lifetime": false,
    "infinite_idle_token_lifetime": false,
    "token_lifetime": 31557600,
    "idle_token_lifetime": 2592000,
    "rotation_type": "rotating"
  },
  "allowed_clients": [],
  "allowed_logout_urls": [
    "https://test.rmi.co.uk/logout",
    "http://localhost:4200",
    "http://localhost:4201",
    "http://localhost:4200/auth/logout",
    "http://localhost:3100",
    "http://localhost:3000",
    "http://localhost:3100/auth/logout",
    "http://localhost:3000/auth/logout"
  ],
  "callbacks": [
    "https://test.rmi.co.uk/authsucess",
    "http://localhost:4200/authsuccess",
    "http://localhost:4201/authsuccess",
    "http://localhost:4200/auth/conclave/callback",
    "http://localhost:3100/authsuccess",
    "http://localhost:3000/authsuccess",
    "http://localhost:3100/auth/oauth/authorize",
    "http://localhost:3000/auth/oauth/authorize",
    "https://www.preprod.rmi-paas.ai-cloud.uk/auth/oauth/authorize"
  ],
  "initiate_login_uri": "https://test.rmi.co.uk/login",
  "native_social_login": {
    "apple": {
      "enabled": false
    },
    "facebook": {
      "enabled": false
    }
  },
  "cross_origin_authentication": true,
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 36000,
    "secret_encoded": false
  },
  "client_aliases": [],
  "token_endpoint_auth_method": "client_secret_post",
  "app_type": "regular_web",
  "grant_types": [
    "authorization_code",
    "implicit",
    "refresh_token",
    "client_credentials"
  ],
  "custom_login_page_on": true
}
