{
  "is_token_endpoint_ip_header_trusted": false,
  "name": "DigiTS",
  "is_first_party": true,
  "oidc_conformant": true,
  "sso_disabled": false,
  "cross_origin_auth": false,
  "refresh_token": {
    "expiration_type": "non-expiring",
    "leeway": 0,
    "infinite_token_lifetime": true,
    "infinite_idle_token_lifetime": true,
    "token_lifetime": 31557600,
    "idle_token_lifetime": 2592000,
    "rotation_type": "non-rotating"
  },
  "allowed_clients": [],
  "allowed_logout_urls": [
    "http://localhost:8080/auth/logout",
    "http://localhost:9090/auth/logout",
    "https://ccs-digits-dev.crowncommercial.gov.uk/auth/logout",
    "https://ccs-portal-dev.crowncommercial.gov.uk/auth/logout",
    "https://ccs-digits-qas.crowncommercial.gov.uk/auth/logout",
    "https://ccs-portal-qas.crowncommercial.gov.uk/auth/logout"
  ],
  "callbacks": [
    "https://et2-business-thetrainline.ttlnonprod.com/corporatesamllogin.aspx",
    "http://localhost:8080/authsuccess",
    "http://localhost:9090/authsuccess",
    "http://localhost:8080/auth/oauth/authorize",
    "http://localhost:9090/auth/oauth/authorize",
    "https://ccs-digits-dev.crowncommercial.gov.uk/authsuccess",
    "https://ccs-portal-dev.crowncommercial.gov.uk/authsuccess",
    "https://ccs-digits-dev.crowncommercial.gov.uk/auth/oauth/authorize",
    "https://ccs-portal-dev.crowncommercial.gov.uk/auth/oauth/authorize",
    "https://ccs-digits-qas.crowncommercial.gov.uk/authsuccess",
    "https://ccs-portal-qas.crowncommercial.gov.uk/authsuccess",
    "https://ccs-digits-qas.crowncommercial.gov.uk/auth/oauth/authorize",
    "https://ccs-portal-qas.crowncommercial.gov.uk/auth/oauth/authorize"
  ],
  "native_social_login": {
    "apple": {
      "enabled": false
    },
    "facebook": {
      "enabled": false
    }
  },
  "addons": {
    "samlp": {
      "mappings": {
        "email": "email",
        "corpref": "corpref"
      },
      "passthroughClaimsWithNoMapping": false,
      "mapUnknownClaimsAsIs": true,
      "mapIdentities": false,
      "createUpnClaim": false
    }
  },
  "cross_origin_authentication": true,
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 36000,
    "secret_encoded": false
  },
  "client_aliases": [],
  "token_endpoint_auth_method": "client_secret_post",
  "app_type": "regular_web",
  "grant_types": [
    "authorization_code",
    "implicit",
    "refresh_token",
    "client_credentials"
  ],
  "custom_login_page_on": true
}
